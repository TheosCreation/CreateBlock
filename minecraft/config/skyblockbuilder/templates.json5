  // Example: 
  // {
  //     "name": "default",
  //     "desc": "",
  //     "file": "default.nbt",
  //     "spawns": "default",
  //     "direction": "south",
  //     "offset": [ 0, 0 ],
  //     "offsetY": 0,
  //     "surroundingMargin": 0,
  //     "surroundingBlocks": "default"
  // }
  "spawn": {
    "name": "Spawn",
    "desc": "",
    "file": "spawn.nbt",
